{
  "name": "cosmos-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "VITE_APP_ENV=staging vite",
    "start-preprod": "VITE_APP_ENV=preprod vite",
    "start-prod": "VITE_APP_ENV=production vite",
    "build": "VITE_APP_ENV=production vite build",
    "build-staging": "VITE_APP_ENV=staging vite build",
    "build-preprod": "VITE_APP_ENV=preprod vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json,scss} --config ./.prettierrc",
    "preview": "vite preview",
    "prepare": "husky install",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@react-oauth/google": "^0.11.0",
    "@reduxjs/toolkit": "^1.9.5",
    "antd": "^5.4.2",
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.0",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0-beta.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.0",
    "prettier": "2.8.8",
    "vite": "^4.3.0"
  }
}
